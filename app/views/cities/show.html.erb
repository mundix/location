<h2>Ciudades de <%= @country.name %>, <%= @city.name %></h2>
<div align="center">
  <% @city.sectors.each do |sector| %>
      <ul class="listing">
        <div class="row">
          <div class="well col-md-4 col-md-offset-4">
            <li class="article-title">
              <span><%= link_to "#{sector.name}", "#" %></span>
            </li>
          </div>
        </div>
      </ul>
  <% end %>
</div>